docker run \
  --rm \
  --network=host \
  -v `pwd`:/home/jovyan/work \
  -v /var/run/docker.sock:/var/run/docker.sock \
  -v /tmp:/tmp \
  --hostname localhost \
  --workdir /home/jovyan/work \
  rpbs/seamless start-notebook.sh  \
  --ip='0.0.0.0' --NotebookApp.token='' --NotebookApp.password='' \
  $*
